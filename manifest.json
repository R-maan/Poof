{
    "name": "Poof",
    "description": "Hides the updated files in vendor and resources folder in GitHub pull requests.",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {"16": "poof.png"}
    },
    "icons": {
      "16": "poof.png"
    } 
  }